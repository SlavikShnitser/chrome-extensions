{
	"manifest_version": 2,
	"name": "Google Maps Context Menu",
	"version": "0.1",

	"icons": {
		"16": "assets/icon.png",
		"48": "assets/icon.png",
		"128": "assets/icon.png"
	},

	"permissions": [
		"activeTab"
	],

	"content_scripts": [{
		"matches": ["https://www.google.com/maps/*"],
		"js": ["js/content.js"]
	}]
}